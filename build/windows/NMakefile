	$(SOURCE)/config.h $(SOURCE)/target.h $(SOURCE)/version.h
getopt.obj: $(SOURCE)/getopt.c $(SOURCE)/getopt.h $(SOURCE)/config.h \
	$(SOURCE)/target.h
