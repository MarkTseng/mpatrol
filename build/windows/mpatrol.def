; DLL Definition


LIBRARY MPATROL

VERSION 1.1

DESCRIPTION "controls and traces dynamic memory allocations"

EXPORTS
	__mp_init	@1
	__mp_fini	@2
	__mp_alloc	@3
	__mp_strdup	@4
	__mp_realloc	@5
	__mp_free	@6
	__mp_setmem	@7
	__mp_copymem	@8
	__mp_comparemem	@9
	__mp_locatemem	@10
	__mp_info	@11
	__mp_memorymap	@12
	__mp_summary	@13
	__mp_check	@14
	__mp_prologue	@15
	__mp_epilogue	@16
	__mp_nomemory	@17
